public class OverloadingDemo {
    // Method overload by increasing and decreasing the number of parameters
    public int sum(int a, int b) {
        return a + b;
    }

    public int sum(int a, int b, int c) {
        return a + b + c;
    }

    // Method overload by changing the data types of parameters
    public double sum(double a, double b) {
        return a + b;
    }

    // Method overload by interchanging the parameters
    public int sum(int b, int a) {
        return a + b;
    }

    public static void main(String[] args) {
        OverloadingDemo demo = new OverloadingDemo();

        // Overload by increasing and decreasing the number of parameters
        System.out.println("Sum of 5 and 7: " + demo.sum(5, 7));
        System.out.println("Sum of 5, 7, and 10: " + demo.sum(5, 7, 10));

        // Overload by changing the data types of parameters
        System.out.println("Sum of 3.5 and 2.5: " + demo.sum(3.5, 2.5));

        // Overload by interchanging the parameters
        System.out.println("Sum of 10 and 15 (interchanged parameters): " + demo.sum(10, 15));
    }
}
