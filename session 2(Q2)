class MyClass {
    int a;
    int b;

    // Default constructor
    MyClass() {
        this.a = 10;
        this.b = 15;
    }

    // Parameterized constructor
    MyClass(int a, int b) {
        this.a = a;
        this.b = b;
    }

    // Method to display values of a and b
    void disp() {
        System.out.println("a = " + a + ", b = " + b);
    }
}

public class Main {
    public static void main(String[] args) {
        MyClass obj1 = new MyClass(); // Calls default constructor
        obj1.disp(); // Display values using default constructor
        
        MyClass obj2 = new MyClass(20, 25); // Calls parameterized constructor
        obj2.disp(); // Display values using parameterized constructor
    }
}
