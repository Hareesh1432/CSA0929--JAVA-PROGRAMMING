class Base {
    // Method to calculate and print area of a square
    void area(int a, int b) {
        int area = a * a;
        System.out.println("Area of square: " + area);
    }
}

class Derived extends Base {
    // Override the method to calculate and print area of a rectangle
    @Override
    void area(int length, int breadth) {
        int area = length * breadth;
        System.out.println("Area of rectangle: " + area);
    }
}

public class Main {
    public static void main(String[] args) {
        Base baseObj = new Base();
        baseObj.area(5, 5); // Calls area() of Base class, prints area of square

        Derived derivedObj = new Derived();
        derivedObj.area(4, 6); // Calls area() of Derived class, prints area of rectangle
    }
}
